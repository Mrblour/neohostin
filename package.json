    "watch:css": "npx tailwindcss -i ./src/input.css -o ./assets/vendor/tailwind/tailwind.css --watch",
    "build:css": "npx tailwindcss -i ./src/input.css -o ./assets/vendor/tailwind/tailwind.css --minify",
    "serve": "npx live-server . --port=${PORT:-8080} --host=0.0.0.0 --wait=1000 --no-browser",
    "serve:local": "npx live-server . --port=8080 --host=localhost --wait=1000",
    "start": "node server.js",
    "dev": "concurrently \"npm run watch:css\" \"npm run serve:local\""
  },
